#include "main.h"
/**
 * _memcpy - function that copy memory area
 * @dest: memory where stored
 * @dest: memory where to be copied
 * @n: number of bytes
 * Return: dest
 */
char *_memcpy(char *dest, char *src, unsigned int n)
{
        int r = 0;
        int i = n;

        for (; r < i; r++)
        {
                dest[r] = src[r];
                n--;
        }
        return (dest);
}
